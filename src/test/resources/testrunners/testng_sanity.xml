<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Opencart App Test Sanity Suite" thread-count="4"
	parallel="tests" verbose="4">


	<listeners>
		<listener
			class-name="com.qa.rockers.listeners.ExtentReportListener" />
		<listener
			class-name="com.qa.rockers.listeners.TestAllureListener" />

		<listener
			class-name="com.qa.rockers.listeners.AnnotationTransformer" />
	</listeners>

	<test name="Login Page Testing">
		<classes>
			<class name="com.qa.rockers.tests.LoginPageTest" />
		</classes>
	</test>
	


</suite>